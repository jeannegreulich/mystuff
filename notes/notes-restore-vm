# Copy the disks and xml to the new server.
cp /var/backup/jmgserver220/2018-08-08.155323/* /var/VM/jmgserver220
# create the machine from the config xml backup:
virsh define --file /var/VM/jmgserver220/jmgserver220.xml
# make sure permsionsion are ok:
chown root:kvm   /var/VM/jmgserver220
chmod 775 /var/VM/jmgserver220
chown root:kvm   /var/VM/jmgserver220/*.xml
chown qemu:qemu /var/VM/jmgserver220/Disk.qcow2
chmod 664 /var/VM/jmgserver220/*

# start the machine
virsh start jmgserver220
# make sure machie starts when host rebooted
virsh  autostart jmgserver220


